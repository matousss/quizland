{
  "name": "quizland-gql",
  "version": "1.0.0",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "gentypes": "graphql-codegen --config ./codegen.yml",
    "watch": "nodemon --experimental-specifier-resolution=node ./start.ts",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node ./start.ts",
    "start": "node --experimental-specifier-resolution=node ./dist/start.js",
    "compile": "tsc",
    "build": "graphql-codegen --config ./codegen.yml && tsc",
    "bundle": "graphql-codegen --config ./codegen.yml && webpack"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "google-auth-library": "^8.7.0",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.16",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^4.13.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/typescript": "^2.8.5",
    "@graphql-codegen/typescript-resolvers": "^2.7.10",
    "@types/node": "18.11.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.4",
    "resolve-tspaths": "^0.8.3",
    "ts-loader": "^9.4.2",
    "tsc-alias": "^1.8.2",
    "tspath": "^2.6.8",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "license": "MIT"
}
